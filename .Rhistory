# Set seed for reproducibility
set.seed(123)
# Generate synthetic time series data
# 1. Low-frequency component (long-term trend)
low_freq_series <- cumsum(rnorm(500))
# 2. High-frequency component (rapid oscillations)
high_freq_series <- sin(2 * pi * 25 * (1:500) / 500) + rnorm(500, sd = 0.2)
# 3. Dominant periodic component
periodic_series <- sin(2 * pi * 10 * (1:500) / 500) + rnorm(500, sd = 0.5)
# 4. White noise (no dominant frequency)
white_noise <- rnorm(500)
# Function to plot time series and its spectral density
plot_time_series_and_sdf <- function(ts_data, title) {
par(mfrow = c(2, 1))
# Plot time series
plot(ts_data, type = 'l', col = 'blue', main = paste(title, "- Time Series"), xlab = 'Time', ylab = 'Value')
# Plot spectral density
spectrum(ts_data, main = paste(title, "- Spectral Density Function"), xlab = 'Frequency', ylab = 'Spectral Density', col = 'red')
}
# Plot for low-frequency series
plot_time_series_and_sdf(low_freq_series, "Low-Frequency Component")
# Plot for high-frequency series
plot_time_series_and_sdf(high_freq_series, "High-Frequency Component")
# Plot for dominant periodic component
plot_time_series_and_sdf(periodic_series, "Dominant Periodic Component")
# Plot for white noise series
plot_time_series_and_sdf(white_noise, "White Noise")
