     &        Atd,Ahol,AAO,ALS,ATC,Aso,Asea,Ausr,Nct2,Lt2,Rgv2,Nr2,
     &        Ktc2,Ncusr2,Nrusrx2,Irfx2,Nintv2,Nextv2,Mxdfl2,
     &        Mxarl2,Mxmal2,Acyc
      LOGICAL Flltd,Fxa,Fnhol,FnAO,FnLS,FnTC,Fnusr,Pktd2,Regfx2,Lma2,
     &        Lar2
      DIMENSION Ap2(PARIMA),Clptr(0:PB),Gptr(0:PGRP),G2(0:PGRP),Bb(PB),
     &          Fxa(PARIMA),Lt2(12),Rgv2(PB),Regfx2(PB),Chx2(PXPX),
     &          Chg2(PGPG),Acm2(PLEN+2*PORDER,PARIMA)
